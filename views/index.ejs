<!DOCTYPE html>
<html lang="en">

<head>
    <title>Web App</title>
    <link rel="stylesheet" href="mystyles.css">
</head>

<body>

    <%- include('header') %>
        <main>
            <div class="grid-row">
                <% mydata.forEach((toy)=> { %>
                    <div class="grid-item">
                        <%= toy.title.slice(0, 30) %>
                            <div class="button">
                                <a
         href="<%= toy.url %>">
                                <button type="button" class="btn btn-primary btn-sm">buy</button>

                                </a>
                            </div>

                    </div>
                    <% }) %>
            </div>
        </main>
        <%- include('footer') %>



</html>